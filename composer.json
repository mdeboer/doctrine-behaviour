{
    "name": "mdeboer/doctrine-behaviour",
    "description": "Library of different entity behaviours (timestampable etc.)",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "mdeboer\\DoctrineBehaviour\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "mdeboer\\DoctrineBehaviour\\Test\\": "test/",
            "mdeboer\\DoctrineBehaviour\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Maarten de Boer",
            "email": "info@maartendeboer.net"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-intl": "*",
        "doctrine/orm": "^2.11",
        "symfony/clock": "^6.4|^7.0"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "phpunit/phpunit": "^10.1|^11.0",
        "symfony/cache": "^5.4|^6.0|^7.0",
        "friendsofphp/php-cs-fixer": "^3.69",
        "squizlabs/php_codesniffer": "^3.11"
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "lock": false
    }
}
